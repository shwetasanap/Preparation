
if (!widgetData3.isEmpty()) {

			
			TeamViewDefinitionInfo teamViewDefinition = new TeamViewDefinitionInfo();
			for (Map.Entry<List<String>, List<List<String>>> entry : widgetData3.entrySet()) {
				String key = entry.getKey().get(0);
				if (entry.getValue().isEmpty()) {
					

					List<ViewDefinitionInfo> data = new ArrayList<>();
					teamViewDefinition.setLoginId(entry.getKey().get(1));
					teamViewDefinition.setCaseDetails(data);
					teamMemberViewDefinitionInfo.add(teamViewDefinition);

				} else {
					teamViewDefinition.setLoginId(entry.getKey().get(1));
					List<ViewDefinitionInfo> data = dashboardMapper.mapTeamViewData(widgetData3.get(entry.getKey()), dashboardViewMappingList);
					teamViewDefinition.setCaseDetails(data);
					teamMemberViewDefinitionInfo.add(teamViewDefinition);
				}

			}

		}
